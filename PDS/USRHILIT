         kw.16   = ' INTERFACE '
         kw.17   = ' MAP '
         kw.18   = ' PACKAGE '
         kw.19   = ' RANGE '
         kw.20   = ' RETURN '
         kw.21   = ' SELECT '
         kw.22   = ' STRUCT '
