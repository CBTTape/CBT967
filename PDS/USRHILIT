         kw.62   = ' &XMODE '
         kw.63   = ' CODE '
         kw.64   = ' DATACLAS '
         kw.65   = ' DO '
         kw.66   = ' ELSE '
         kw.67   = ' END '
         kw.68   = ' EXCLUDE '
         kw.69   = ' EXIT '
         kw.70   = ' FILTLIST '
         kw.71   = ' IF '
         kw.72   = ' INCLUDE '
         kw.73   = ' MGMTCLAS '
         kw.74   = ' OTHERWISE '
         kw.75   = ' PROC '
         kw.76   = ' SELECT '
         kw.77   = ' SET '
         kw.78   = ' STORCLAS '
         kw.79   = ' STORGRP '
         kw.80   = ' THEN '
         kw.81   = ' WHEN '
         kw.82   = ' WRITE '
         /* Highlite & variables in Pink */
         do i = 1 to 62
            clr.i = copies('P',(length(kw.i)-2))
         end
      end
   when (usrhilng = 'CARLA') then
      do
