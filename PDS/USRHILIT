         kw.31   = ' &LIBNAME '
         kw.32   = ' &LLQ '
         kw.33   = ' &MAXGENS '
         kw.34   = ' &MAXSIZE '
         kw.35   = ' &MEMHLQ '
         kw.36   = ' &MEMLLQ '
         kw.37   = ' &MEMN '
