         kw.48   = ' &RETPD '
         kw.49   = ' &SECLABL '
         kw.50   = ' &SECOND_QTY '
         kw.51   = ' &SIZE '
         kw.52   = ' &SPACE_TYPE '
         kw.53   = ' &STEP '
         kw.54   = ' &STORCLAS '
         kw.55   = ' &STORGRP '
